<div>
    <label>Name</label>
    <input type="text" name="name" value="<%= workplace.name %>">
</div>
<div>
    <label>Address</label>
    <input type="text" name="address" value="<%= workplace.address %>">
</div>
<div>
    <label>Phone</label>
    <input type="tel" name="phone" value="<%= workplace.mobile %>">
</div>
<div>
    <label>E-mail</label>
    <input type="email" name="email" value="<%= workplace.email %>">
</div>
<div>
    <label>Description</label>
    <input type="text" name="description" value="<%= workplace.description %>">
</div>
