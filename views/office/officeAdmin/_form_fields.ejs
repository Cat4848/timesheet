<div class="form-row">
    <section class="form-item">
        <label for="name">Name*</label>
        <input type="text" id="name" name="name" value="<%= officeAdmin.name %>" autocomplete="name" required>
    </section>
    <section class="form-item">
        <label for="email">E-mail*</label>
        <input type="email" id="email" name="email" value="<%= officeAdmin.email%>" autocomplete="email" required>
    </section>
    <section class="form-item">
        <label for="password">Password*</label>
        <input type="password" id="password" name="password" value="<%= officeAdmin.password %>" autocomplete="new-password" required>
    </section>
    <section>
        <input type="hidden" name="permission" value="office">
    </section>
</div>